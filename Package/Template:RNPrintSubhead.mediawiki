<!--

	adding link to PE index pages

-->{{Link-AnywhereElse
	|product=ReleaseNotes
	|version=Current
	|manual=GenesysEngage-cloud
	|topic={{#replace:{{{DTName}}}| |}}
	|display text={{{DTName}}}}}: {{{ReleaseNumber|}}} available {{#time:F d, Y|{{#if:{{{PEDate|}}}|{{{PEDate|}}}|{{{MixedDate|}}} }} }}
</li><!--  

	print link to helm chart page, if exists 

-->{{#cargo_query:
	tables=
		ArticleCloudRN,
		ArticleCloudRN__ServiceId,
		ArticleHelmRN__ServiceId,
		ArticleHelmRN
	|join on=
		ArticleCloudRN._ID=ArticleCloudRN__ServiceId._rowID,
		ArticleCloudRN__ServiceId._value=ArticleHelmRN__ServiceId._value,
		ArticleHelmRN__ServiceId._rowID=ArticleHelmRN._ID
	|fields=CONCAT('<li>[[',ArticleHelmRN._pageName,'|Helm charts and containers]]</li>')
	|where=
		ArticleCloudRN._pageName="{{FULLPAGENAME}}" 
		AND ArticleHelmRN._pageNamespace="{{NAMESPACENUMBER}}"
	|default=
	|delimiter=
}}
